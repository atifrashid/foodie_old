<?xml version='1.0' encoding='utf-8'?>
<widget id="com.foodiesys.foodiemobileapp" version="1.5.0" versionCode="1040300" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Fudi</name>
    <description>Authentic and Specialized Cuisine by Local Chefs</description>
    <author email="alex@foodiesys.com" href="http://www.FoodieSys.com">Alex Gray</author>
    <preference name="permissions" value="none" />
    <access origin="*" />
    <allow-intent href="*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="http://mobileapi.foodiesys.com/*" />
    <allow-navigation href="data:*" />
    
	<preference name="phonegap-version" value="cli-6.3.0" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="android-minSdkVersion" value="17" />
    <preference name="android-targetSdkVersion" value="23" />
    <preference name="android-versionCode" value="1040300" />
    <preference name="android-build-tool" value="gradle" />
    <gap:plugin name="cordova-plugin-geolocation" source="npm" spec="2.2.0" />
    <gap:plugin name="com.paypal.cordova.mobilesdk" source="git" spec="https://github.com/paypal/PayPal-Cordova-Plugin#8115ac6cbbc2a320b652f52b5d338c8e04e3882e" />
    <plugin name="cordova-plugin-whitelist" />
    <plugin name="cordova-plugin-camera" source="npm" spec="2.2.0" />
    <plugin name="cordova-plugin-media-capture" source="pgb" />
    <plugin name="cordova-plugin-file" source="pgb" />
    <plugin name="cordova-plugin-file-transfer" source="pgb" />
    <plugin name="cordova-plugin-console" source="pgb" />
    <gap:plugin name="phonegap-plugin-barcodescanner" source="npm" />
    <icon src="icon.png" />
    <platform name="ios">
        <icon height="57" src="foodie-logo_57x57.png" width="57" />
        <icon height="72" src="foodie-logo_72x72.png" width="72" />
        <icon height="76" src="foodie-logo_76x76.png" width="76" />
        <icon height="120" src="foodie-logo_120x120.png" width="120" />
        <icon height="152" src="foodie-logo_152x152.png" width="152" />
        <icon height="512" src="foodie-logo_512x512.png" width="512" />
        <icon height="1024" src="foodie-logo_1024x1024.png" width="1024" />
    </platform>
    <platform name="android" />
    <platform name="winphone" />
    <plugin source="git" spec="https://github.com/jeduan/cordova-plugin-facebook4#aff9a08a86cc6c6a18019f7adc2896ddf97c11cd">
        <param name="APP_ID" value="1429053297422142" />
        <param name="APP_NAME" value="Foodie" />
    </plugin>
    <engine name="android" spec="~5.1.1" />
</widget>